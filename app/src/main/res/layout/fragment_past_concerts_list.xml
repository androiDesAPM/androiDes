            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="40dp"
            android:text="Fecha del concierto"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/concertCountry2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="60dp"
            android:text="País"
            android:textColor="#000000"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/cameraIcon2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@android:drawable/ic_menu_camera"
            app:tint="#000000" />

        <ImageButton
            android:id="@+id/concertPhotos2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="45dp"
            app:srcCompat="@android:drawable/ic_menu_gallery"
            app:tint="#000000" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="335dp"
        android:layout_height="105dp"
        android:layout_marginBottom="244dp"
        android:background="#6A939393"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/concertName3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"
            android:text="Nombre del concierto"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/concertDate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="40dp"
            android:text="Fecha del concierto"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/concertCountry3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="60dp"
            android:text="País"
            android:textColor="#000000"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/cameraIcon3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@android:drawable/ic_menu_camera"
            app:tint="#000000" />

        <ImageButton
            android:id="@+id/concertPhotos3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="45dp"
            app:srcCompat="@android:drawable/ic_menu_gallery"
            app:tint="#000000" />

    </FrameLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>