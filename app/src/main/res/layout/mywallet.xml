<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#00BCD4">

    <Button
        android:id="@+id/button4"
        android:layout_width="149dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:backgroundTint="#4DDAEC"
        android:text="Mis entradas"
        android:textAllCaps="false"
        android:textColor="#0E0D0D"
        android:textSize="14sp"
        app:icon="@drawable/tickets"
        app:iconPadding="5dp"
        app:iconSize="30dp"
        app:iconTint="#090909"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="638dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="158dp"
        android:layout_height="55dp"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="#4DDAEC"
        android:text="Mis monedas"
        android:textAllCaps="false"
        android:textColor="#0E0D0D"
        android:textSize="14sp"
        app:icon="@drawable/coins"
        app:iconGravity="textStart"
        app:iconSize="30dp"
        app:iconTint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.696"
        app:layout_constraintStart_toEndOf="@+id/button5"
        tools:layout_editor_absoluteY="638dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="35dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="135dp"
        android:layout_height="23dp"
        android:layout_marginTop="39dp"
        android:text="Nombre de usuario"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="200dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="email@example.com"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="78dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        android:text="Mis monedas"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:text="Donar a un artista"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="302dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:text="Artista:"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="339dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:text="monedas"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="386dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:text="Cantidad:"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="386dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="101dp"
        android:layout_height="35dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="7dp"
        android:background="#E1E4E6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="   1000"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="73dp"
        android:layout_height="29dp"
        android:layout_marginStart="144dp"
        android:background="#E1E4E6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="   100"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="378dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="135dp"
        android:layout_height="30dp"
        android:background="#E1E4E6"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="   Nombre del artista"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="332dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="74dp"
        android:layout_height="42dp"
        android:layout_marginStart="276dp"
        android:backgroundTint="#4DDAEC"
        android:text="Donar"
        android:textAllCaps="false"
        android:textColor="#0E0D0D"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="432dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="45dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/coins"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="221dp" />

    <View
        android:id="@+id/divider"
        android:layout_width="409dp"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="288dp" />

    <View
        android:id="@+id/divider2"
        android:layout_width="409dp"
        android:layout_height="2dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#252222"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="490dp" />

</androidx.constraintlayout.widget.ConstraintLayout>